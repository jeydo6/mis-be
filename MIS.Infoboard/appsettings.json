{
	"ConnectionStrings": {
		"DefaultConnection": "Server=Server;Database=MIS;Persist Security Info=True;User ID=user;Password=123456;"
	},
	"Serilog": {
		"MinimumLevel": {
			"Default": "Error",
			"Override": {
				"Microsoft": "Warning",
				"System": "Warning"
			}
		},
		"Using": [ "Serilog.Sinks.File" ],
		"WriteTo": [
			{
				"Name": "File",
				"Args": {
					"path": "D:\\Logs\\MIS\\MIS.Infoboard-.txt",
					"rollingInterval": "Day",
					"outputTemplate": "{Timestamp:o} [{Level:u3}] {Message}{NewLine}{Exception}"
				}
			}
		]
	}
}