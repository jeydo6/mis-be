<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:userControls="using:MIS.Infoboard.UserControls"
             mc:Ignorable="d" d:DesignWidth="1920" d:DesignHeight="1080"
             x:Class="MIS.Infoboard.UserControls.MainControl"
             FontFamily="/Assets/Fonts#Cambria">
	<DockPanel>
		<userControls:HeaderControl DockPanel.Dock="Top" Height="75" Margin="0,10" />
		<userControls:FooterControl DockPanel.Dock="Bottom" Height="35" Margin="0,10" />
		<userControls:CarouselControl />
	</DockPanel>
</UserControl>
